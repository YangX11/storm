<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whyxs.mapper.system.SysDictMapper">


    <select id="getAreaListByParent" parameterType="string" resultType="map">
      select `id` AS `value`,`name` AS `key` from sys_dict_area where pid=#{pid}
    </select>

</mapper>
